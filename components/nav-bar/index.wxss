@import "../../theme.wxss";

.nav-bar {
  background-color: var(--bgColor);
  height: 88rpx;
  box-sizing: content-box;
  text-align: center;
  position: relative;
  line-height: 88rpx;
}

.outline {
  border-bottom: 1rpx solid var(--borderColor);
}

.back {
  color: #000;
  width: 88rpx;
  font-size: 40rpx;
  line-height: 88rpx;
  display: inline-block;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
}
