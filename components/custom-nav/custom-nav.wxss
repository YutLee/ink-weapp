.custom-nav {
  background-color: #fff;
  height: 88rpx;
  box-sizing: content-box;
  text-align: center;
  position: relative;
  line-height: 88rpx;
  border-bottom: 1rpx solid #e5e5e5;
}

.icon-before {
  color: #000;
  width: 88rpx;
  font-size: 40rpx;
  line-height: 88rpx;
  display: inline-block;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 0;
}
